{
  "license": "ISC",
  "version": "1.0.0",
  "main": "src/server.js",
  "type": "module",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cloudinary": "^1.36.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.15.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "mongoose-paginate-v2": "^1.7.1",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2"
  },
  "scripts": {
    "test": "set NODE_ENV=development&&node -r dotenv-safe/config src/test.js",
    "start": "set NODE_ENV=development&&node -r dotenv-safe/config src/server.js",
    "dev": "set NODE_ENV=development&&nodemon -r dotenv-safe/config src/server.js",
    "prepare": "husky install",
    "lint:fix": "eslint --fix --ext .js . --ignore-path .gitignore",
    "prettier:fix": "prettier --ignore-path .gitignore -w \"**/*.js\"",
    "lint": "yarn prettier:fix && yarn lint:fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/node": "^18.15.7",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7"
  },
  "lint-staged": {
    "**/*.js": [
      "yarn run lint",
      "git add ."
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
